@startuml

skinparam classAttributeIconSize 0

class User {
    -isAuthenticated : bool
    -username : string
    -password : string
    +isAuthenticated() : bool
    -roles : List<Role>
    +hasPermission(Permission) : bool
    -contactInformation : ContactInformation
    -birthday : Date
    -doNotVisit : bool
    -personalCalendar : PersonalCalendar
    -ward : Ward
    -preferences : Preferences 
}
@enduml
